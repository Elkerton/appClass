<!DOCTYPE html>
<html lang="pt-br">
<head> 
    <title>AppClass</title>
    <meta charset="UTF-8">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <link href='https://fonts.googleapis.com/css?family=Droid+Sans' rel='stylesheet' type='text/css'>
    <link rel="stylesheet" href="../static/style.css">
    </head>
<body>
    
  
  <div class="content">
      <!--Titulo e logo AppClass  -->
      <center>
        <div class="row">
             <h1 class="col-md-4 col-md-offset-4" id="title">AppClass</h1>
        </div>
      </center>
      <!--Fim Titulo e logo AppClass-->

      <!--Input usuário-->
      <center>
          <div class="row">
              <span class="col-md-4 col-md-offset-4">Usuário</span>
              <input type="text" class="col-md-2 col-md-offset-5" id="user">
          </div>
      </center>
      <!--Fim Input usuário-->

      <!--Input senha-->
      <center>
        <div class="row">
          <span class="col-md-4 col-md-offset-4">Senha</span>
          <input type="password" class="col-md-2 col-md-offset-5" id="password">
        </div>
      </center>
      <!--Fim input senha-->

      <!--Botao login-->
      <div class="row">
          <div class="col-md-2 col-md-offset-5" id="btns">
            <button class="btn btn-success" id="btn-login">Login</button>
             <button class="btn btn-default" id="btn-sign" >Cadastre-se</button>
          </div>
      </div>
      <!--Fim botao login-->

      
 </div>



</body>
</html>